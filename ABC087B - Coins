using System;
class program{
	static void Main(){
		int c500 = int.Parse(Console.ReadLine());
		int c100 = int.Parse(Console.ReadLine());
		int c50 = int.Parse(Console.ReadLine());
		int X = int.Parse(Console.ReadLine());
		int cnt=0;
		for(int i=0;i<=c500;i++){
			for(int j=0;j<=c100;j++){
				for(int k=0;k<=c50;k++){
					int chk = 500*i + 100*j + 50*k;
					if(chk==X){
						cnt++;
					}
				}
			}
		}
		Console.WriteLine(cnt);
	}
}
